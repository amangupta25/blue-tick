{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "default": {
      "cleverLinks": true,
      "monospaceLinks": false
    }
  },
  "opts": {
    "destination": "./public/documentation",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "templates/default"
  }
}
